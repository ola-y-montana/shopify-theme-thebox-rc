<script>
  defer(function(){
    try {
     const $sliderWrapper = $('#SlideshowWrapper-{{ section.id }}')

     const $slideshow = $sliderWrapper.find('.slideshow')
     
      $slideshow.on('beforeChange', function(event, slick, currentSlide, nextSlide){
        const $currentSlide = $slideshow.find('[data-slick-index="'+ nextSlide +'"]')
        const $slideVideo = $currentSlide.find('video')
        if ($slideVideo.length) {
          $slideVideo[0].play()
        }
      });                                    

    } catch(error) {
      console.log(error)
    }
  }, function() { return window.jQuery })
</script>
{% endif %}

